struct Point {
    var x: Double
    var y: Double
    // Структуры передаются по значению
    // Нет поддержки наследования
    // Члены инициализируются автоматически
}

class PointClass {
    var x: Double
    var y: Double
    // Классы передаются по ссылке
    // Поддерживают наследование
    // Имеют более широкие возможности управления доступом
    init(x: Double, y: Double) {
        self.x = x
        self.y = y
    }
}
